<div id="admin-new">
<div class="box3">
  <div class="box-inner">
    <%= form_with model: @admin, url: session_path(resource_name) do |f| %>
    <h1 class="login">ログイン</h1>
    <div class="box-email-login">
      <%= f.label :メールアドレス %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control" %>
    </div>
    <div class="box-password">
      <%= f.label :パスワード %><br />
      <%= f.password_field :password, autocomplete: "current-password", class: "form-control" %>
    </div>
    <div class="d-grid">
      <%= f.submit "ログイン", class: "btn btn-success" %>
    </div>
    <% end %>
  </div>
</div>
</div>