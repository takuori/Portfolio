<% if post.liked_by?(current_member) %>
  <%= link_to post_likes_path(post), method: :delete, remote: true do %>
    <i class="fas fa-heart favorite1"></i>
    <%= post.likes.count %>
  <% end %>
<% else %>
  <%= link_to post_likes_path(post), method: :post, remote: true do %>
    <i class="fas fa-heart favorite2"></i>
    <%= post.likes.count %>
  <% end %>
<% end %>


  
  